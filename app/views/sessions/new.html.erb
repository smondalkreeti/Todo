<%= render(partial: "shared/error_messages", locals: { target: @user }) %>
<%= form_for(@user, url: sessions_create_path) do |f| %>
  <div>
    <%= f.label :email, "Email" %>
    <%= f.email_field :email, placeholder: "Enter Email" %>
  </div>
    <%= f.label :password, "Password" %>
    <%= f.password_field :password, placeholder: "Enter password" %>
  </div>
  <div>
    <%= f.submit "Sign In" %>
  </div>
<% end %>

<%= link_to "Sign Up", signup_path %>
